{
  "name": "@jeremyqzt/nodestats",
  "version": "1.1.4",
  "description": "Helper methods for stats, combinatorics, permutations and probabilities",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "test-travis": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "gen-docs": "jsdoc ./src/ -d ./docs -R ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremyqzt/statsJs.git"
  },
  "keywords": [
    "zscore",
    "ztable",
    "permutations",
    "combinations",
    "combinatorics",
    "statistics",
    "stats",
    "probabilities",
    "percentiles"
  ],
  "author": "Jeremy Qian and Ben Ni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremyqzt/statsJs/issues"
  },
  "homepage": "https://github.com/jeremyqzt/statsJs#readme",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "jsdocs": "^1.0.0",
    "mocha": "^8.0.1",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {}
}
